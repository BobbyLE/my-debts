<h1>Listing iowethems</h1>

<table>
  <thead>
    <tr>
      <th>Amount</th>
      <th>Person</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @iowethems.each do |iowethem| %>
      <tr>
        <td><%= iowethem.amount %></td>
        <td><%= iowethem.person %></td>
        <td><%= link_to 'Show', iowethem %></td>
        <td><%= link_to 'Edit', edit_iowethem_path(iowethem) %></td>
        <td><%= link_to 'Destroy', iowethem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Iowethem', new_iowethem_path %>
